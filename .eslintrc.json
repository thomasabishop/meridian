{
   "root": true,
   "env": {
      "node": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "prettier"],
   "plugins": ["@typescript-eslint"],
   "overrides": [],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "rules": {
      "no-console": "off"
   },
   "ignorePatterns": ["out", "dist", "**/*.d.ts"]
}
